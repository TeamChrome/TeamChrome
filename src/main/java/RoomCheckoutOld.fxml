<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Scene?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<Scene xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="src.main.java.RoomCheckoutController">
   <root>
      <AnchorPane prefHeight="400.0" prefWidth="600.0">
         <children>
            <TextArea fx:id="roomTextArea" editable="false" layoutX="28.0" layoutY="57.0" prefHeight="200.0" prefWidth="200.0" />
            <Text layoutX="28.0" layoutY="39.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Room Checkout">
               <font>
                  <Font size="19.0" />
               </font>
            </Text>
            <Button fx:id="roomCheckoutBackButton" layoutX="28.0" layoutY="350.0" mnemonicParsing="false" onAction="#goBackToRoomSearch" text="Back" />
            <Button fx:id="roomCheckoutReserveButton" layoutX="508.0" layoutY="350.0" mnemonicParsing="false" onAction="#reserveRoom" text="Reserve" />
         </children>
      </AnchorPane>
   </root>
</Scene>
